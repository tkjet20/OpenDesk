@model IEnumerable<UPS.SpaceX.Core.DeskDto>

<select id="DropDownAvailableDesks">
    @foreach (UPS.SpaceX.Core.DeskDto desk in Model)
    {
        <option value="@desk.Id">@desk.Name</option>
    }
</select>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Type</th>
            <th scope="col">DESK</th>
            <th scope="col">DATE</th>
        </tr>
    </thead>
    <tbody>


        @foreach (UPS.SpaceX.Core.DeskDto desk in Model)
        {
            <tr>
                <th scope="row">Active</th>
                <td value="@desk.Id">@desk.Name</td>
            </tr>
        }

    </tbody>
</table>
